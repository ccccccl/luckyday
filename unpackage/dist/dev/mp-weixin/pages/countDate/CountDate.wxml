<view class="{{['container','_div','data-v-641152a2',virtualHostClass]}}" style="{{virtualHostStyle}}"><view class="row _div data-v-641152a2"><view class="_div data-v-641152a2">开始日期:</view><picker class="val data-v-641152a2" mode="date" value="{{startDateString}}" data-event-opts="{{[['change',[['onDateChange',['$event']]]]]}}" bindchange="__e">{{''+startDateString+''}}</picker></view><view class="row _div data-v-641152a2"><view class="_div data-v-641152a2">间隔：</view><input class="val data-v-641152a2" type="number" placeholder="请输入间隔数字" data-event-opts="{{[['input',[['onItervalInput',['$event']]]]]}}" value="{{interval}}" bindinput="__e"/></view><view class="row _div data-v-641152a2"><view class="_div data-v-641152a2">单位：</view><picker class="val data-v-641152a2" mode="selector" range="{{units}}" range-key="text" value="{{unitIndex}}" data-event-opts="{{[['change',[['onUnitChange',['$event']]]]]}}" bindchange="__e">{{''+units[unitIndex].text+''}}</picker></view><view class="row _div data-v-641152a2"><view class="_div data-v-641152a2">计算方向：</view><picker class="val data-v-641152a2" mode="selector" range="{{directionArr}}" value="{{directionIndex}}" data-event-opts="{{[['change',[['onDirectionChange',['$event']]]]]}}" bindchange="__e">{{''+directionArr[directionIndex]+''}}</picker></view><view class="row _div data-v-641152a2"><view class="_div data-v-641152a2">计算结果:</view><view class="val _div data-v-641152a2">{{''+endDateString+''}}</view></view></view>